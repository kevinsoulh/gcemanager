# Feature Flags
VITE_USE_FIREBASE=true                       # Using Firestore for storage
VITE_USE_GOOGLE_CALENDAR=false               # Using mock calendar for now

# Mock Settings
VITE_MOCK_MEET_LINK=https://meet.google.com/mock-link

# Firebase Configuration (Replace with your values from Firebase Console)
VITE_FIREBASE_API_KEY=your-api-key-here
